<!doctype html>
<!--[if IE 9]> <html class="ie9 no-js" lang="{{ shop.locale }}"> Karl Home 4 <![endif]-->
<html class="no-js{% if settings.enable_rtl == true %} lang-rtl{% endif %}" lang="{{ shop.locale }}" {% if settings.enable_rtl == true %} dir="rtl" {% endif %}>
<head>
  <meta name="facebook-domain-verification" content="s26vsrffgww6anpq3ihkk7vc9hzfzi" />
  <meta charset="utf-8">
  <meta http-equiv="Cache-Control" content="private, no-cache, no-store, must-revalidate">
  <meta http-equiv="Expires" content="0">
  <meta http-equiv="Pragma" content="no-cache">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  
  {% if settings.favicon != blank %}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {% endif %}

  {% capture seo_title %}
    {{ page_title }}
    {% if current_tags %}
      {%- assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
    {% endif %}
    {% if current_page != 1 %}
      &ndash; {{ 'general.meta.page' | t: page: current_page }}
    {% endif %}
    {% unless page_title contains shop.name %}
      &ndash; {{ shop.name }}
    {% endunless %}
  {% endcapture %}
  <title>{{ seo_title }}</title>

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}">
  {% endif %}

  {{ content_for_header }}

  {% render 'judgeme_core' %}

  <style type="text/css">
    @font-face {
      font-family: "Material-Design-Iconic-Font";
      src: url("https://cdn.shopify.com/s/files/1/0737/6496/2598/files/Material-Design-Iconic-Font.woff2?v=1686311353") format("woff2");
      font-display: swap; /* or any other value you choose */
    }
    @font-face {
      font-family: "Quicksand";
      src: url("https://cdn.shopify.com/s/files/1/0737/6496/2598/files/Quicksand-Light.woff2?v=1686307452") format("woff2");
      font-display: swap; /* or any other value you choose */
    }
    @font-face {
      font-family: "Nobile";
      src: url("https://cdn.shopify.com/s/files/1/0737/6496/2598/files/Nobile-Regular.woff2?v=1684359477") format("woff2");
      font-display: swap; /* or any other value you choose */
    } 


  </style>

  <script>
    function loadCSS(href) {
      var cssLink = document.createElement('link');
      cssLink.rel = 'stylesheet';
      cssLink.href = href;
      document.head.appendChild(cssLink);
    }

    loadCSS('{{ 'material-design-iconic-font.min.css' | asset_url }}');


  </script>
  
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
  {% include 'nov-head-css' %}
  {% include 'nov-head-js' %}
  {% include 'social-meta-tags' %}
  {% include 'css-variables' %}

  {% comment %}
  {% if template.name == 'collection' or template.name == 'product' %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js" async></script>
  {% else %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  {% endif %}
  {% endcomment %}
  {% if template != index %}
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  {% endif %}
  <script src="{{ 'shopify_global.js' | asset_url }}" defer></script>
  <script src="{{ 'dwytfixes.js' | asset_url }}"></script>

  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Arvo:ital,wght@0,400;0,700;1,400;1,700&family=Lato:wght@300;400;700;900&display=swap" rel="stylesheet">
  {% comment %}
    font-family: 'Arvo', serif;
    font-family: 'Lato', sans-serif; 
  {% endcomment %}


  {% capture body_template %}
      {{content_for_layout}}
  {% endcapture %}

  {% if body_template contains 'hero-block' %}
    {{ 'hero-block.css' | asset_url | stylesheet_tag }}
    {{ 'hero-block.js' | asset_url | script_tag }}
  {% endif %} 

  {% if body_template contains 'swiper' %}
    <script src="https://cdn.jsdelivr.net/npm/swiper@8.4.7/swiper-bundle.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@8.4.7/swiper-bundle.min.css">
  {% endif %}
  
  {% if body_template contains 'reviews-slider' %}
    {{ 'reviews-slider.css' | asset_url | stylesheet_tag }}
    {{ 'reviews-slider.js' | asset_url | script_tag }}
  {% endif %}

  {% if body_template contains 'video-block' %}
    {{ 'video-block.css' | asset_url | stylesheet_tag }}
    {{ 'video-block.js' | asset_url | script_tag }}
  {% endif %}


</head>

</html>

  <body class="template-{{ template | split: '.' | first }} {{ page_title | handle }}">
    {% comment %}
    {% if template.name != 'index' %}
      <div class="preloader_nov">
        <div class="nov_load"></div>
      </div>
    {% endif %}
    {% endcomment %}
    <a class="in-page-link visually-hidden skip-link" href="#MainContent">
      {{- 'general.accessibility.skip_to_content' | t -}}
    </a>
    <div class="layout_content position-relative">
      <div class="account-list dropdown-menu dropdown-menu-right" id="_desktop_account_list">
        <div class="close_account d-md-flex d-none align-items-center {% if settings.logo_tabbar != blank %}justify-content-between{% else %}justify-content-center{% endif %}">
          {% if settings.logo_tabbar != blank %}
            <a href="/">
              <img
                class="img-fluid logo_tabbar lazyload"
                data-src="{{ settings.logo_tabbar | img_url: '100x' }}"
                alt="{{ shop.name }}"
                width="{{ image.width }}"
                height="{{ image.height }}"
              >
            </a>
          {% endif %}
          <i class="zmdi zmdi-close"></i>
        </div>
        <div class="nov_sideward_content">
          <div class="account-list-content">
            {% if shop.customer_accounts_enabled %}
              {% if customer %}
                <div>
                  <a
                    class="myaccount"
                    href="{{ shop.secure_url }}/account"
                    rel="nofollow"
                    title="{{ 'layout.customer.account' | t }}"
                  >
                    <span>{{ 'layout.customer.account' | t }}</span>
                  </a>
                </div>
                <div>
                  <a
                    class="logout"
                    href="{{ shop.secure_url }}/account/logout"
                    rel="nofollow"
                    title="{{ 'layout.customer.log_out' | t }}"
                  >
                    <span>{{ 'layout.customer.log_out' | t }}</span>
                  </a>
                </div>
              {% else %}
                <div>
                  <a
                    class="login"
                    href="{{ shop.secure_url }}/account/login"
                    rel="nofollow"
                    title="{{ 'layout.customer.log_in' | t }}"
                  >
                    <span>{{ 'layout.customer.log_in' | t }}</span>
                  </a>
                </div>
                {% if shop.customer_accounts_optional %}
                  <div>
                    <a
                      class="register"
                      href="{{ shop.secure_url }}/account/register"
                      rel="nofollow"
                      title="{{ 'layout.customer.create_account' | t }}"
                    >
                      <span>{{ 'layout.customer.create_account' | t }}</span>
                    </a>
                  </div>
                {% endif %}
              {% endif %}
            {% endif %}
            <div>
              <a
                class="check-out"
                href="{{ shop.secure_url }}/checkout"
                rel="nofollow"
                title="{{ 'layout.customer.checkout' | t }}"
              >
                <span>{{ 'layout.customer.checkout' | t }}</span>
              </a>
            </div>
            {% if settings.wishlist_enable %}
              <div>
                <a
                  class="wishlist"
                  href="/pages/page-wishlist"
                  rel="nofollow"
                  title="{{ 'layout.customer.wishlists' | t }}"
                >
                  <span>{{ 'layout.customer.wishlists' | t }}</span>
                </a>
              </div>
            {% endif %}
          </div>
        </div>
        <div class="currency_header d-none d-md-block">
          <div class="heading">{{ 'layout.currency.title' | t }}</div>
          {% include 'nov-block-currency' %}
        </div>
      </div>
      {% if settings.mini_cart_type == 'canvas' %}
        <div class="cart_canvas_content">
          <div id="_desktop_cart">
            <div class="close_cart d-md-flex d-none align-items-center {% if settings.logo_tabbar != blank %}justify-content-between{% else %}justify-content-center{% endif %}">
              {% if settings.logo_tabbar != blank %}
                <a href="/">
                  <img
                    class="img-fluid logo_tabbar lazyload"
                    data-src="{{ settings.logo_tabbar | img_url: '100x' }}"
                    alt=""
                    width="{{ image.width }}"
                    height="{{ image.height }}"
                  >
                </a>
              {% endif %}
              <i class="zmdi zmdi-close"></i>
            </div>
            <div id="cart-info"></div>
          </div>
        </div>
      {% endif %}

      {% section 'nov-header' %}

      <main class="main-content" id="MainContent">
        <section class="page-container" id="PageContainer">
          {{ content_for_layout }}
        </section>
      </main>

      {% comment %} {% section 'nov-footer' %} {% endcomment %}
      {% section 'footer-new' %}
    </div>
    <div class="canvas-menu drawer-left">
      <div class=" canvas-header-box d-flex justify-content-center align-items-center">
        <div class="close-box"><i class="zmdi zmdi-close"></i></div>
      </div>
      {% comment %}
      <div class="menu-footer">
        <div class="mf-item">
          <a href="https://aliasgoodskin.com/account/register">
            <svg xmlns="http://www.w3.org/2000/svg" id="Interface_B_out" height="100" viewBox="0 0 100 100" width="100" data-name="Interface B out"><path d="m50 48.67c12.23 0 22.18-9.95 22.18-22.18s-9.95-22.18-22.18-22.18-22.18 9.95-22.18 22.18 9.95 22.18 22.18 22.18z"/><path d="m18.91 94.89h62.19c1.91 0 3.72-.83 4.97-2.29 1.22-1.43 1.76-3.31 1.47-5.15-2.92-18.57-18.7-32.05-37.53-32.05s-34.61 13.48-37.53 32.05c-.29 1.84.24 3.72 1.47 5.15 1.24 1.46 3.05 2.29 4.97 2.29z"/></svg>
              <span class="bb"><p>Accounts</p></span>
            </a>
        </div>
      </div>
      {% endcomment %}
    </div>
    <div id="mobile-blockcart" class="mobile-boxpage d-flex d-md-none test">
        <div class="content-boxpage col">
          <div class="box-header d-flex justify-content-between align-items-center">
              <div class="title-box">{{ 'sticky_mobile.cart.title' | t }}</div>
              <div class="close-box">{{ 'sticky_mobile.close.title' | t }}</div>
          </div>
          <div id="_mobile_cart" class="box-content"></div>
        </div>
    </div>                 
    <div class="canvas-overlay"></div>
    <div class="sidebar-overlay"></div>
    {% include 'nov-formsearch' %}
    {% include 'nov-search-autocomplete' %}
    {% comment %}{% include 'nov-stickymenu-mobile' %}{% endcomment %}
    {% include 'nov-popup-newsletter' %}
    {% if settings.wishlist_enable %}
      {% include 'nov-popup-wishlist' %}
    {% endif %}

    <div id="_desktop_back_top">
      <div id="back-top">
        <span>
          <i class="zmdi zmdi-chevron-up"></i>
        </span>
      </div>
    </div>

    <div class="link_to_collection_cart_empty d-none">
      <ul class="list-unstyled">
        {% if settings.collec_1_name != blank %}
          <li class="mb-10">
            <a class="btn" href="{{ settings.collec_1 }}"
              ><span>{{ settings.collec_1_name }}</span></a
            >
          </li>
        {% endif %}
        {% if settings.collec_2_name != blank %}
          <li class="mb-10">
            <a class="btn" href="{{ settings.collec_2 }}"
              ><span>{{ settings.collec_2_name }}</span></a
            >
          </li>
        {% endif %}
        {% if settings.collec_3_name != blank %}
          <li class="mb-10">
            <a class="btn" href="{{ settings.collec_3 }}"
              ><span>{{ settings.collec_3_name }}</span></a
            >
          </li>
        {% endif %}
        {% if settings.collec_4_name != blank %}
          <li class="mb-10">
            <a class="btn" href="{{ settings.collec_4 }}"
              ><span>{{ settings.collec_4_name }}</span></a
            >
          </li>
        {% endif %}
        {% if settings.collec_5_name != blank %}
          <li class="mb-10">
            <a class="btn" href="{{ settings.collec_5 }}"
              ><span>{{ settings.collec_5_name }}</span></a
            >
          </li>
        {% endif %}
      </ul>
    </div>

    <div id="Form_newletter" class="form-vertical modal fade" tabindex="-1" role="dialog" aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header position-initial">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <i class="zmdi zmdi-hc-fw zmdi-close"></i>
            </button>
          </div>
          <div class="modal-body">
            <div class="block_title text-center">
              <div class="title">{{ 'general.newsletter_form.title_popup_soldout' | t }}</div>
              <div class="sub">{{ 'general.newsletter_form.sub_popup_soldout' | t }}</div>
            </div>
            {% form 'customer' %}
              {{ form.errors | default_errors }}
              {% if form.posted_successfully? %}
                <p class="form--success text-center" style="color: #fff;">
                  {{ 'general.newsletter_form.confirmation' | t }}
                </p>
              {% else %}
                <div class="input-group d-block">
                  <input type="hidden" name="contact[tags]" value="newsletter">
                  <input
                    type="email"
                    name="contact[email]"
                    id="Email_newsletter"
                    class="input-group__field newsletter__input text-center"
                    value="{% if customer %}{{ customer.email }}{% endif %}"
                    placeholder="{{ 'general.newsletter_form.email_placeholder_soldout' | t }}"
                  >
                  <button type="submit" class="btn newsletter__submit w-100" name="commit" id="Subscribe_newsletter">
                    <span class="newsletter__submit d-flex align-items-center justify-content-center hover_x">
                      <span>
                        <i class="zmdi zmdi-email"></i>
                        <span>{{ 'general.newsletter_form.noitify_me' | t }}</span>
                      </span>
                      <i class="zmdi zmdi-long-arrow-right"></i>
                    </span>
                  </button>
                </div>
                <div class="checkbox">
                  <label id="myCheck" class="d-inline-flex">
                    <span class="custom-checkbox">
                      <input name="no-view" class="no-view" type="checkbox">
                      <span class="ps-shown-by-js"><i class="zmdi zmdi-check"></i></span>
                    </span>
                    <span id="text">
                      {{ 'general.newsletter_form.note' | t }}
                      <a href="/pages/term-and-services">Terms of Service</a>
                      and
                      <a href="/pages/privacy-policy">Privacy Policy</a>
                    </span>
                  </label>
                </div>
              {% endif %}
            {% endform %}
          </div>
        </div>
      </div>
    </div>
    {% if settings.show_size_chart %}
      <div id="Size_chart" class="form-vertical modal fade" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
          <div class="modal-content">
            <div class="modal-header position-initial">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <i class="zmdi zmdi-hc-fw zmdi-close"></i>
              </button>
            </div>
            <div class="modal-body">
              <div class="single__tabs">
                <div class="block_nav d-flex align-items-center">
                  <ul class="nav nav-tabs">
                    {% if settings.size_image_1 != blank %}
                      <li>
                        <a
                          class="active"
                          href="#womenTabs1"
                          data-toggle="tab"
                          style="color: {{ settings.color_text_tab_1 }}"
                        >
                          {{- settings.text_tab_1 -}}
                        </a>
                      </li>
                    {% endif %}
                    {% if settings.size_image_2 != blank %}
                      <li>
                        <a
                          {% if settings.size_image_1 == blank %}
                            class="active"
                          {% endif %}
                          href="#menTabs2"
                          data-toggle="tab"
                          style="color: {{ settings.color_text_tab_2 }}"
                        >
                          {{- settings.text_tab_2 -}}
                        </a>
                      </li>
                    {% endif %}
                  </ul>
                </div>
                <div class="tab-content">
                  {% if settings.size_image_1 != blank %}
                    <div class="tab-pane active" id="womenTabs1">
                      <div class="block_img">
                        <img
                          src="{{ settings.size_image_1 | img_url: '1170x' }}"
                          alt="img"
                          width="{{ image.width }}"
                          height="{{ image.height }}"
                        >
                      </div>
                    </div>
                  {% endif %}
                  {% if settings.size_image_2 != blank %}
                    <div class="tab-pane {% if settings.size_image_1 == blank %}active{% endif %}" id="menTabs2">
                      <div class="block_img">
                        <img
                          src="{{ settings.size_image_2 | img_url: '1170x' }}"
                          alt="img"
                          width="{{ image.width }}"
                          height="{{ image.height }}"
                        >
                      </div>
                    </div>
                  {% endif %}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    {% endif %}

    {% include 'nov-currency' %}
{% comment %}
