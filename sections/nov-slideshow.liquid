<div class="distance" {% if section.settings.custom_bg != blank %}style="background:url('{{ section.settings.custom_bg | img_url: 'master'}}'"{% endif %}>
  {% if section.settings.has_fullwidth == false %}
  <div class="container_90">
  {% else %}
    <div class="container_xl">
  {% endif %}
  <div data-section-id="{{ section.id }}" data-section-type="slideshow-section" class="slideshow">
    {% if section.blocks.size > 0 %}
      <div class="main-slider" data-autoplay="{{ section.settings.autoplay }}" data-speed="6000" data-arrows="{{ section.settings.arrows }}" data-dots="{{ section.settings.dots }}">
        {% for block in section.blocks %}
          {% if block.type == 'image' %}
            {% if block.settings.image != blank %}
             <div class="item image">
                <figure>
                  {% if block.settings.slider_link != blank %}
                  <a href="{{ block.settings.slider_link }}">
                  {% endif %}
                    {% if block.settings.image != blank %}
                      <div class="slide-image slide-media" style="background-image:url('{{ block.settings.image | img_url: '2048x' }}'); background-size: {{ block.settings.image-display }}; background-position:{{ block.settings.image-position }}; background-repeat: no-repeat;"></div>
                    {% else %}
                      <img src="https://via.placeholder.com/1920x950">
                    {% endif %}
                  {% if block.settings.slider_link != blank %}
                  </a>
                  {% endif %}
                  <figcaption class="caption">
                    <div class="content-caption content-captiont-{{ forloop.index }} {% if block.settings.text_align == 'left' %}rtl-left {% elsif block.settings.text_align == 'right' %}rtl-right {% else %}rtl-center{% endif %}">
                      <div class="content position-absolute content-{{ forloop.index }}">
                        {% unless block.settings.caption_1 == blank %}
                          <div class="caption-animate caption-1 mb-xs-10" data-animate="{{ block.settings.cap_animate_1 }} animated" style="color: {{ block.settings.color_cap1 }};">{{ block.settings.caption_1 | nofilter }}</div>
                        {% endunless %}
                        {% unless block.settings.caption_2 == blank %}
                          <div class="title caption-animate caption-2 mb-xs-20" data-animate="{{ block.settings.cap_animate_2 }} animated" style="color: {{ block.settings.color_cap2 }};">{{ block.settings.caption_2 | nofilter }}</div>
                        {% endunless %}
                        {% unless block.settings.caption_3 == blank %}
                          <div class="caption-animate caption-3" data-animate="{{ block.settings.cap_animate_3 }} animated" style="color: {{ block.settings.color_cap3 }};">{{ block.settings.caption_3 | nofilter }}</div>
                        {% endunless %}
                        {% unless block.settings.caption_4 == blank %}
                          <div class="caption-animate caption-4" data-animate="{{ block.settings.cap_animate_4 }} animated" style="color: {{ block.settings.color_cap4 }};">{{ block.settings.caption_4 | nofilter }}</div>
                        {% endunless %}
                        {% unless block.settings.cap_link == blank %}
                          <div class="caption-animate cap_link hover_x" data-animate="{{ block.settings.cap_link_animate }} animated" style="background: {{ block.settings.background_cap_link }}">
                            <a href="{{ block.settings.cap_link }}" style="color: {{ block.settings.color_cap_link }}; border: 2px solid {{ block.settings.color_cap_link }}">
                              <span>{{ block.settings.title_link }}</span>
                              <i class="zmdi zmdi-long-arrow-right"></i>
                            </a>
                          </div>
                        {% endunless %}
                      </div>
                    </div>
                  </figcaption>
                </figure>
              </div> 
            {% endif %}
            {% if block.settings.image == blank %}
            <div class="item image blank">
              <figure>
                {% capture current %}{% cycle 1, 2 %}{% endcapture %}
                {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
                <figcaption class="caption-animate caption {% if block.settings.type_caption == '1' %}type_1{else}type_2{% endif %}">
                  {% unless block.settings.caption_1 == blank %}
                    <div class="caption-animate caption-1"data-animate="bounceInRight">{{ block.settings.caption_1 | nofilter }}</div>
                  {% endunless %}
                  {% unless block.settings.caption_2 == blank %}
                    <div class="caption-animate caption-2" data-animate="bounceInLeft">{{ block.settings.caption_2 | nofilter }}</div>
                  {% endunless %}
                  {% unless block.settings.caption_3 == blank %}
                    <div class="caption-animate caption-3" data-animate="bounceInRight">{{ block.settings.caption_3 | nofilter }}</div>
                  {% endunless %}
                  {% unless block.settings.caption_4 == blank %}
                    <div class="caption-animate caption-4" data-animate="bounceInLeft">{{ block.settings.caption_4 | nofilter }}</div>
                  {% endunless %}
                </figcaption>
              </figure>
              </div>  
            {% endif %}
          {% endif %}
          {% if block.type == 'video' %}
            {% if block.settings.type_video == 'video_youtobe' %}
              <div class="item youtube">
                <iframe allow='autoplay' class="embed-player slide-media" width="980" height="520" src="https://www.youtube.com/embed/{{ block.settings.video_url_youtobe.id }}?autoplay=1" frameborder="0" allowfullscreen></iframe>
              </div>
            {% endif %}
            {% if block.settings.type_video == 'video_links' %}
              <div class="item youtube">
                <video playsinline preload autoplay loop muted style="width: 100%;max-width: 100%; height: auto;">
                  <source src="{{ block.settings.video_url_links }}" type="video/mp4" />
                </video>
              </div>
            {% endif %}
          {% endif %}
        {% endfor %}
      </div>
    {% endif %}
    {% if section.blocks.size == 0 %}
      <div class="placeholder-noblocks">
        {{ 'homepage.onboarding.no_content' | t }}
      </div>
    {% endif %}
  </div>
  </div>
</div>
<style>
@media(max-width: 767px){
  .slick-slide{
    margin: 0 !important;
  }
  .slideshow{
    background: none !important;
  }
}
</style>
                    
