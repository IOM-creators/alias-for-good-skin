{% if section.settings.background_color != blank %}
  <style>
    .footer-layout , .footer_copyright {
      background-color: {{ section.settings.background_color }};
    }
  </style>
{% endif %}
<footer>
  <div class="footer-layout">
    <div class="container_ct">
      <div class="row">
        {% for block in section.blocks %}
          {% if block.type == 'block_menu' %}
            {%- assign footer_linklist = block.settings.menu_handle -%}
            <div class="footer_menu footer-block footer-block__{{ forloop.index }} {{ block.settings.width_xl_desktop }} {{ block.settings.width_lg_desktop }} {{ block.settings.width_md_tablet }} {{ block.settings.width_sm_mobile }} col-xs-12 {{ block.settings.block_class }}">
              {% if block.settings.block_title != blank %}
                <div class="title-block d-xs-flex align-items-center justify-content-between "  style="{% if block.settings.color_title != blank %}color: {{ block.settings.color_title }};{% endif %}">
                  <span>{{ block.settings.block_title }}</span>
                  <span class="show_footer d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                </div>
              {% endif %}
              <div class="block-content{% if block.settings.block_title != blank %} adc{% endif %}">
                {% if linklists[footer_linklist].links.size > 0 %}
                  <ul class="site-footer__linklist list-unstyled">
                    {% for link in linklists[footer_linklist].links %}
                      <li class="site-footer__linklist-item">
                        <a href="{{ link.url }}" style="{% if block.settings.color_menu != blank %}color: {{ block.settings.color_menu }};{% endif %}">{{ link.title }}</a>
                      </li>
                    {% endfor %}
                  </ul>
                {% endif %}
              </div>
            </div>
          {% endif %}
          {% if block.type == 'block_html' %}
            <div class="footer_html footer-block footer-block__{{ forloop.index }} {{ block.settings.width_xl_desktop }} {{ block.settings.width_lg_desktop }} {{ block.settings.width_md_tablet }} {{ block.settings.width_sm_mobile }} col-xs-12 {{ block.settings.block_class }}">
              {% if block.settings.block_title != blank %}
                <div class="title-block d-xs-flex align-items-center justify-content-between "  style="{% if block.settings.color_title != blank %}color: {{ block.settings.color_title }};{% endif %}">
                  <span>{{ block.settings.block_title }}</span>
                  <span class="show_footer d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                </div>
              {% endif %}
              {% if block.settings.html_content != blank %}
                <div class="block-content{% if block.settings.block_title != blank %} adc{% endif %}">{{ block.settings.html_content }}</div>
              {% endif %}
            </div>
          {% endif %}
          {% if block.type == 'block_newsletter' %}
            <div class="footer_newsletter footer-block footer-block__{{ forloop.index }} {{ block.settings.width_xl_desktop }} {{ block.settings.width_lg_desktop }} {{ block.settings.width_md_tablet }} {{ block.settings.width_sm_mobile }} col-xs-12 {{ block.settings.block_class }}">
              {% if block.settings.block_title != blank %}
                <div class="title-block d-xs-flex align-items-center justify-content-between " style="{% if block.settings.color_title != blank %}color: {{ block.settings.color_title }};{% endif %}">
                  <span>{{ block.settings.block_title }}</span>
                  <span class="show_footer d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                </div>
              {% endif %}
              {% if block.settings.newsletter_des != blank %}
                <div class="des" style="{% if block.settings.color_des != blank %}color: {{ block.settings.color_des }};{% endif %}">{{ block.settings.newsletter_des | nofilter }}</div>
              {% endif %}
              <div class="block-content{% if block.settings.block_title != blank %} adc{% endif %}">
                {% form 'customer' %}
                  {{ form.errors | default_errors }}
                  {% if form.posted_successfully? %}
                    <p class="form--success">{{ 'general.newsletter_form.confirmation' | t }}</p>
                  {% else %}
                    <div class="input-group">
                      <input type="hidden" name="contact[tags]" value="newsletter">
                      <input type="email"
                        name="contact[email]"
                        id="Email_Footer"
                        class="input-group__field newsletter__input form-control"
                        value="{% if customer %}{{ customer.email }}{% endif %}"
                        placeholder="{{ 'general.newsletter_form.placeholder_footer' | t }}">
                    </div>
                    <div class="input-group__btn">
                      <button type="submit" class="btn newsletter__submit" name="commit" id="Subscribe_footer">
                        <span class="hover_x newsletter__submit-text--large">
                          <span>{{ 'general.newsletter_form.submit_footer' | t }}</span>
                          <i class="zmdi zmdi-long-arrow-right"></i>
                        </span>
                      </button>
                    </div>
                  {% endif %}
                {% endform %}
              </div>
              <div class="social_footer">
                {% include 'nov-social' %}
              </div>
            </div>     
          {% endif %} 
          {% if block.type == 'block_contact' %}
            <div class="footer_contact footer-block footer-block__{{ forloop.index }} {{ block.settings.width_xl_desktop }} {{ block.settings.width_lg_desktop }} {{ block.settings.width_md_tablet }} {{ block.settings.width_sm_mobile }} col-xs-12 {{ block.settings.block_class }}">
              {% if block.settings.block_title != blank %}
                <div class="title-block d-xs-flex align-items-center justify-content-between "  style="{% if block.settings.color_title != blank %}color: {{ block.settings.color_title }};{% endif %}">
                  <span>{{ block.settings.block_title }}</span>
                  <span class="show_footer d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                </div>
              {% endif %}
              {% if block.settings.logo_footer != blank %}
                <div class="logo_footer">
                  <img class="" src="{{ block.settings.logo_footer | img_url: '234x52' }}" alt="" width="auto" height="auto" loading="lazy">
                </div>
              {% else %}
                <div class="logo_footer">
                  <img src="https://via.placeholder.com/134x27" alt="" width="auto" height="auto" loading="lazy">
                </div>
              {% endif %}
              <div class="block-content">
                {% if block.settings.contact_address != blank %}
                  <div class="contact address" style="{% if block.settings.color_address != blank %}color: {{ block.settings.color_address }};{% endif %}">{{ block.settings.contact_address }}</div>
                {% endif %}
                {% if block.settings.contact_phone != blank %}
                  <div class="contact phone" style="{% if block.settings.color_phone != blank %}color: {{ block.settings.color_phone }};{% endif %}">{{ block.settings.contact_phone }}</div>
                {% endif %}
                {% if block.settings.contact_email != blank %}
                  <div class="contact email" style="{% if block.settings.color_email != blank %}color: {{ block.settings.color_email }};{% endif %}">{{ block.settings.contact_email }}</div>
                {% endif %}
                {% if block.settings.contact_hours != blank %}
                  <div class="contact open" style="{% if block.settings.color_hours != blank %}color: {{ block.settings.color_hours }};{% endif %}">{{ block.settings.contact_hours }}</div>
                {% endif %}
              </div>
              {% include 'nov-social' %}
            </div>
          {% endif %}
        {% endfor %} 
      </div>
    </div>
  </div>

  <div class="footer_copyright">
    <div class="container_ct">
      <div class="d-md-flex align-items-center {% if section.settings.menu_copyright != blank %}justify-content-between{% else %}justify-content-center{% endif %} d-xs-block text-xs-center d-sm-block text-sm-center">
        {% if section.settings.menu_copyright != blank %}
          <div class="copyright_right d-flex align-items-center justify-content-sm-center justify-content-xs-center mb-xs-30">
            {%- assign footer_linklist = section.settings.menu_copyright -%}
            {% if linklists[footer_linklist].links.size > 0 %}
              <ul class="site-footer__linklist list-unstyled d-flex align-items-center">
                {% for link in linklists[footer_linklist].links %}
                  <li class="site-footer__linklist-item">
                    <a href="{{ link.url }}" style="{% if section.settings.color_menu != blank %}color: {{ section.settings.color_menu }};{% endif %}">{{ link.title }}</a>
                  </li>
                {% endfor %}
              </ul>
            {% endif %}
          </div>
        {% endif %}
        {% if section.settings.payment != blank %}
          <div class="payment_icons mb-xs-30">
            <img src="{{ section.settings.payment | img_url: '240x25' }}" alt="" width="auto" height="auto" loading="lazy">
          </div>
        {% else %}
          <div class="payment_icons"><img src="https://via.placeholder.com/240x25" width="auto" height="auto" loading="lazy"></div>
        {% endif %}
        <div class="copyright" style="{% if section.settings.color_menu != blank %}color: {{ section.settings.color_menu }};{% endif %}">{{ section.settings.copyright }}</div>
      </div>
    </div>
  </div>
</footer>

