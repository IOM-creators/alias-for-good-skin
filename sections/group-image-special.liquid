<div class="distance{% if section.settings.class != blank %} {{ section.settings.class }}{% endif %}" style="padding-top: {{ section.settings.distance_top }}; padding-bottom: {{ section.settings.distance_bottom }}">
  	{% if section.settings.has_fullwidth == false %}
  	<div class="container_90">
  	{% endif %}
	  	<div class="row spacing-{{ section.settings.spacing }}">
	  		{% for block in section.blocks %}
		  		{% if block.type == 'block_image_1' %}
			  		<div class="item_image{% if block.settings.class_image != blank %} {{ block.settings.class_image }}{% endif %} {{ block.settings.width }} {{ block.settings.width_lg }} {{ block.settings.width_md }}">
			  			<div class="gallery-image_gr">
			  				{% if block.settings.show_image_1 %}
		  						<div class="gallery-image__item hover-image mb-{{ section.settings.spacing }} mb-md-20 mb-xs-10">
						  			{% if block.settings.image_1 != blank %}
							  			<div class="respone_image" style="padding-top:{{ 1 | divided_by: block.settings.image_1.aspect_ratio | times: 100}}%;">
							  				{% assign img_url = block.settings.image_1 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
							  				<img class="row__image lazyload"
							  				src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 1'%3E%3C/svg%3E"
							  				data-src="{{ img_url }}"
							  				data-widths="[360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
							  				data-aspectratio="{{ block.settings.image_1.aspect_ratio }}"
							  				data-sizes="auto"
							  				alt="">
							  			</div>
							  			{% if block.settings.cta_label_1 != blank %}
								  			<div class="gallery-image__caption">
								  				<a href="{{ block.settings.link_1 }}" class="hover_x" style="{% if block.settings.color_link_1 != blank %}color: {{ block.settings.color_link_1 }};{% endif %}{% if block.settings.size_link_1 != blank %}font-size: {{ block.settings.size_link_1 }};background: {{ block.settings.background_button_1 }};{% endif %}">
								  					<span>{{ block.settings.cta_label_1 | escape }}</span>
								  					<i class="zmdi zmdi-long-arrow-right"></i>
								  				</a>
								  			</div>
							  			{% endif %}
						  			{% else %}
						  				<img src="https://via.placeholder.com/450x450?text=450x450">
						  			{% endif %}
						  		</div>
					  		{% endif %}
					  		{% if block.settings.show_image_2 %}
						  		<div class="gallery-image__item hover-image mb-{{ section.settings.spacing }} mb-md-20 mb-xs-10">
						  			{% if block.settings.image_2 != blank %}
							  			<div class="respone_image" style="padding-top:{{ 1 | divided_by: block.settings.image_2.aspect_ratio | times: 100}}%;">
							  				{% assign img_url = block.settings.image_2 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
							  				<img class="row__image lazyload"
							  				src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 1'%3E%3C/svg%3E"
							  				data-src="{{ img_url }}"
							  				data-widths="[360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
							  				data-aspectratio="{{ block.settings.image_2.aspect_ratio }}"
							  				data-sizes="auto"
							  				alt="">
							  			</div>
							  			{% if block.settings.cta_label_2 != blank %}
								  			<div class="gallery-image__caption">
								  				<a href="{{ block.settings.link_2 }}" class="hover_x" style="{% if block.settings.color_link_2 != blank %}color: {{ block.settings.color_link_2 }};{% endif %}{% if block.settings.size_link_2 != blank %}font-size: {{ block.settings.size_link_2 }};background: {{ block.settings.background_button_2 }};{% endif %}">
								  					<span>{{ block.settings.cta_label_2 | escape }}</span>
								  					<i class="zmdi zmdi-long-arrow-right"></i>
								  				</a>
								  			</div>
							  			{% endif %}
						  			{% else %}
						  				<img src="https://via.placeholder.com/450x450?text=450x450">
						  			{% endif %}
						  		</div>
					  		{% endif %}
					  	</div>
			  		</div>
		  		{% endif %}
		  		{% if block.type == 'block_image_2' %}
			  		<div class="item_image{% if block.settings.class_image != blank %} {{ block.settings.class_image }}{% endif %} {{ block.settings.width }} {{ block.settings.width_lg }} {{ block.settings.width_md }}">
			  			{% if block.settings.show_image %}
				  			<div class="gallery-image__item hover-image">
					  			{% if block.settings.image != blank %}
						  			<div class="respone_image" style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;">
						  				{% assign img_url = block.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
						  				<img class="row__image lazyload"
						  				src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 1'%3E%3C/svg%3E"
						  				data-src="{{ img_url }}"
						  				data-widths="[360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
						  				data-aspectratio="{{ block.settings.image.aspect_ratio }}"
						  				data-sizes="auto"
						  				alt="">
						  			</div>
						  			{% if block.settings.cta_label != blank %}
							  			<div class="gallery-image__caption">
							  				<a href="{{ block.settings.link }}" class="hover_x" style="{% if block.settings.color_link != blank %}color: {{ block.settings.color_link }};{% endif %}{% if block.settings.size_link != blank %}font-size: {{ block.settings.size_link }};background: {{ block.settings.background_button }};{% endif %}">
							  					<span>{{ block.settings.cta_label | escape }}</span>
							  					<i class="zmdi zmdi-long-arrow-right"></i>
							  				</a>
							  			</div>
						  			{% endif %}
					  			{% else %}
					  				<img src="https://via.placeholder.com/922x922?text=922x922">
					  			{% endif %}
				  			</div>
			  			{% endif %}
			  		</div>
		  		{% endif %}
	  		{% endfor %}
	  	</div>
	{% if section.settings.has_fullwidth == false %} 
  	</div>
  	{% endif %}
</div>

